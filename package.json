{
  "name": "carrott-auth",
  "version": "1.0.0",
  "description": "authentication service for carrott backend",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn audit && jest --coverage"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./test-results/jest",
    "outputName": "results.xml"
  },
  "dependencies": {
    "aws-sdk": "^2.734.0",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.13.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "express-winston": "^4.0.3",
    "get-random-values": "^1.2.0",
    "google-auth-library": "^6.0.6",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.3.1",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-apple": "^1.1.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^5.21.9",
    "socket.io-client": "^2.3.0",
    "socket.io-mock": "^1.3.0",
    "util": "^0.12.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.4.1",
    "jest-junit": "11.1.0",
    "lint-staged": "10.2.13",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "sequelize-mock": "0.10.2",
    "supertest": "4.0.2"
  }
}
